{
  "name": "dbox/uploader-api",
  "description": "Dropbox uploader PHP library",
  "keywords": [
    "dropbox",
    "uploader",
    "dropbox-uploader",
    "file-uploader",
    "dropbox-file-uploader"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksey Romanov",
      "email": "lex-romanow@yandex.ru",
      "homepage": "https://www.linkedin.com/in/romanov-aleksey",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5 || ^8.0",
    "guzzlehttp/guzzle": "^7.10",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.88",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.4",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Dbox\\UploaderApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dbox\\UploaderApi\\Tests\\": "tests"
    }
  },
  "scripts": {
    "cs": "php-cs-fixer fix",
    "stan": "phpstan analyse",
    "unit": "phpunit",
    "unit:cover": "phpunit --coverage-html .phpunit.report"
  },
  "minimum-stability": "stable"
}